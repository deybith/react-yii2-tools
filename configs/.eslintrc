{
  "extends": [
    "standard",
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "rules": {
    "one-var": "off",
    "semi": ["error", "always", { "omitLastInOneLineBlock": true }],
    "space-before-function-paren": ["error", "never"]
  },
  "globals": {
    "fetch": false
  },
  "parser": "babel-eslint"
}